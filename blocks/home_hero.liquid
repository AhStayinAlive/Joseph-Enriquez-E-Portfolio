>
  <div class="peak-video-hero__background-{{ ai_gen_id }}">
    {% if section.settings.desktop_video %}
      <video
        class="peak-video-hero__video-{{ ai_gen_id }}"
        autoplay
        muted
        loop
        playsinline
        {% if section.settings.poster_image %}poster="{{ section.settings.poster_image | image_url: width: 1920}}"{% endif %}
        aria-label="{{ section.settings.video_alt_text | escape }}"
      >
        <source src="{{ section.settings.desktop_video }}" type="video/mp4">
      </video>
    {% endif %}

    {% if section.settings.mobile_image %}
      <img
        class="peak-video-hero__mobile-image-{{ ai_gen_id }}"
        src="{{ section.settings.mobile_image | image_url: width: 1200 }}"
        alt="{{ section.settings.mobile_image.alt | escape }}"
        loading="eager"
        width="{{ section.settings.mobile_image.width }}"
        height="{{ section.settings.mobile_image.height }}"
      >
    {% elsif section.settings.desktop_video == blank %}
      <div class="peak-video-hero__placeholder-{{ ai_gen_id }}">
        {{ 'hero-apparel-1' | placeholder_svg_tag }}
      </div>
    {% endif %}
  </div>

  <div class="peak-video-hero__overlay-{{ ai_gen_id }}"></div>

  <div class="peak-video-hero__container-{{ ai_gen_id }}">
    <div class="peak-video-hero__content-{{ ai_gen_id }}">
      <div class="peak-video-hero__headlines-{{ ai_gen_id }}">
        {% if section.settings.headline_line_1 != blank %}
          <h1
            class="peak-video-hero__headline-{{ ai_gen_id }} peak-video-hero__headline-1-{{ ai_gen_id }}"
            data-animate="{{ section.settings.animate_line_1 }}"data-delay="0">
            {{ section.settings.headline_line_1 }}
          </h1>
        {% endif %}

        {% if section.settings.headline_line_2 != blank %}
          <h1
            class="peak-video-hero__headline-{{ ai_gen_id }} peak-video-hero__headline-2-{{ ai_gen_id }}"
            data-animate="{{ section.settings.animate_line_2 }}"
            data-delay="200"
          >
            {{ section.settings.headline_line_2 }}
          </h1>
        {% endif %}

        {% if section.settings.headline_line_3 != blank %}
          <h1
            class="peak-video-hero__headline-{{ ai_gen_id }} peak-video-hero__headline-3-{{ ai_gen_id }} {% unless section.settings.line_3_same_as_h1 %}peak-video-hero__headline-{{ ai_gen_id }}--subtitle{% endunless %}"
            data-animate="{{ section.settings.animate_line_3 }}"
            data-delay="400"
          >
            {{ section.settings.headline_line_3 }}
          </h1>
        {% endif %}
      </div>

      {% if section.settings.description != blank %}
        <p
          class="peak-video-hero__description-{{ ai_gen_id }}"
          data-animate="{{ section.settings.animate_description }}">
          {{ section.settings.description }}
        </p>
      {% endif %}

      {% assign has_buttons = false %}
      {% for i in (1..8) %}
        {% assign button_label_key = 'button_' | append: i | append: '_label' %}
        {% if section.settings[button_label_key] != blank %}
          {% assign has_buttons = true %}
          {% break %}
        {% endif %}
      {% endfor %}

      {% if has_buttons %}
        <div
          class="peak-video-hero__buttons-{{ ai_gen_id }}"
          data-animate="{{ section.settings.animate_buttons }}"
        >
          {% for i in (1..8) %}
            {% liquid
              assign button_label_key = 'button_' | append: i | append: '_label'
              assign button_url_key = 'button_' | append: i | append: '_url'
              assign button_style_key = 'button_' | append: i | append: '_style'
              assign button_new_tab_key = 'button_' | append: i | append: '_new_tab'
              assign button_aria_key = 'button_' | append: i | append: '_aria_label'

              assign button_label = section.settings[button_label_key]
              assign button_url = section.settings[button_url_key]
              assign button_style = section.settings[button_style_key]
              assign button_new_tab = section.settings[button_new_tab_key]
              assign button_aria = section.settings[button_aria_key]
            %}

            {% if button_label != blank %}<a
                href="{{ button_url | default: '#' }}"
                class="peak-video-hero__button-{{ ai_gen_id }} peak-video-hero__button-{{ ai_gen_id }}--{{ button_style }}"
                {% if button_new_tab %}target="_blank" rel="noopener noreferrer"{% endif %}
                {% if button_aria != blank %}aria-label="{{ button_aria | escape }}"{% endif %}
              >
                {{ button_label }}
              </a>
            {% endif %}
          {% endfor %}
        </div>
      {% endif %}
    </div>
  </div>
</peak-video-hero-{{ ai_gen_id }}>

<script>
  (function() {
    class PeakVideoHero{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.animationDuration = {{ section.settings.animation_duration | default: 800 }};
        this.animationDelay = {{ section.settings.animation_delay | default: 200 }};
      }

      connectedCallback() {
        this.setupAnimations();
        this.setupVideoHandling();
      }

      setupAnimations() {
        if (window.matchMedia('(prefers-reduced-motion: reduce)').matches) {
          return;
        }

        const observer = new IntersectionObserver((entries) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {
              this.triggerAnimations();
              observer.unobserve(entry.target);
            }
          });
        }, { threshold: 0.1 });

        observer.observe(this);
      }

      triggerAnimations() {
        const animatedElements = this.querySelectorAll('[data-animate="true"]');
        
        animatedElements.forEach((element, index) => {
          const delay = parseInt(element.dataset.delay) || (index * this.animationDelay);
          
          setTimeout(() => {
            element.classList.add('animate');
          }, delay);
        });
      }

      setupVideoHandling() {
        const video = this.querySelector('.peak-video-hero__video-{{ ai_gen_id }}');
        if (!video) return;

        video.addEventListener('loadeddata', () => {
          video.play().catch(() => {
            console.log('Video autoplay was prevented');
          });
        });

        video.addEventListener('error', () => {
          const mobileImage = this.querySelector('.peak-video-hero__mobile-image-{{ ai_gen_id }}');
          if (mobileImage) {
            mobileImage.style.display = 'block';
            video.style.display = 'none';
          }
        });
      }
    }

    customElements.define('peak-video-hero-{{ ai_gen_id }}', PeakVideoHero{{ ai_gen_id }});
  })();
</script>

